{
   "background": {
      "scripts": [ "runFlagging.js" ]
   },
   "content_scripts": [ {
      "js": [ "connection.js" ],
      "matches": [ "*://*.salesforce.com/", "*://*.force.com/" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "manifest_version": 2,
   "name": "Salesforce Instance Flag",
   "permissions": [ "tabs", "storage", "http://*/", "https://*/" ],
   "version": "4.2"
}
